{
  "name": "@swan-io/docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "yarn api-reference && docusaurus start --port 3001",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "graphql-update-schemas": "mkdir -p scripts/graphql/dist && node scripts/graphql/downloadSchema.mjs",
    "api-reference": "rm -fr docs/api && docusaurus graphql-to-doc",
    "full-build": "yarn graphql-update-schemas && yarn api-reference && yarn build"
  },
  "dependencies": {
    "@docusaurus/core": "2.3.1",
    "@docusaurus/preset-classic": "2.3.1",
    "@docusaurus/theme-mermaid": "^2.3.1",
    "@docusaurus/theme-search-algolia": "^2.3.1",
    "@graphiql/plugin-explorer": "^0.1.14",
    "@graphiql/toolkit": "^0.8.2",
    "@graphql-markdown/docusaurus": "^1.16.6",
    "@graphql-tools/graphql-file-loader": "^7.5.16",
    "@mdx-js/react": "^1.6.21",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "graphiql": "^2.4.0",
    "graphql": "^16.6.0",
    "node-fetch": "^3.3.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
